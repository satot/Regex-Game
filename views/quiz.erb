<article id="quizapp">
  <h1 id="qnumber">Q1</h1>
  <div id=#quiz>
    <section class="question left">
      <h2>Match Words</h2>
      <ul id="match_list">
      </ul>
    </section>
    <section class="question right">
      <h2>Don't Match Words</h2>
      <ul id="not_match_list">
      </ul>
    </section>
  </div>
  <section id="timer"></section>
  <section class="answer">
    <form id="answer_form" action="/c/<%= @course_id %>/result/complete">
      <div id="answer_box">
        <div class="regex">/^</div><div id="answer_in_box" class="regex">&nbsp;</div><div class="regex">$/</div>
        <!--<input type="hidden" id="reg_input" name="answer"/>-->
      </div>
      <p id="choice_items"></p>
      <p id="submit"><input type="button" value="OK" id="reg_submit" /></p>
    </form>
  </section>
</article>
<script src="/js/underscore.js"></script>
<script src="/js/backbone.js"></script>
<script src="/js/stopwatch.js"></script>
<script src="/js/regexgame.js"></script>
