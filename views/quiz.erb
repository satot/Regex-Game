<article id="quizapp">
  <h1 id="qnumber">Q1</h1>
  <div id=#quiz>
    <section class="question left">
      <h2>Match Words</h2>
      <ul id="match_list">
      </ul>
    </section>
    <section class="question right">
      <h2>Don't Match Words</h2>
      <ul id="not_match_list">
      </ul>
    </section>
  </div>
  <section id="timer"></section>
  <section class="answer">
    <form id="answer_form" action="/c/<%= @course_id %>/result/complete">
      <p><span class="regex">/^</span><input type="text" id="reg_input" name="answer"/><span class="regex">$/</span></p>
      <p><input type="button" value="OK" id="reg_submit" /></p>
    </form>
  </section>
</article>
<script src="/js/underscore-min.js"></script>
<script src="/js/backbone-min.js"></script>
<script src="/js/timer.js"></script>
<script src="/js/regexgame.js"></script>
